#include<stdio.h>
int main(){

int student[3];
int subject[5];
int total=0;

  for(int i=0;i<3;i++){
    printf("enter student roll no :");
    scanf("%d",&student[i]);
     for(int j=0;j<5;j++){
        printf("enter marks :%d",j);
        scanf("%d",&subject[j]);
}
   for(int j=0;j<5;j++){
     printf("%d\n",subject[j]);
     total=subject[j];
     per=total/5;
   }
   printf("total is : %d\n",total);
   printf("per is : %d\n",per);
   if(per>85){
    printf("A grade/n");
 } else if(per<85 && per>65){
    printf("B grade/n");
    } else if(per<65 && per>55){
    printf("B grade/n");
   }
  }















    return 0;
}